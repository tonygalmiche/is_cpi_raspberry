<?xml version="1.0" encoding="utf-8"?><openerp><data>    <record id="is_report_rebuts_tree_view" model="ir.ui.view">        <field name="name">is_report_rebuts_tree_view</field>        <field name="model">is.report.rebuts</field>        <field name="arch" type="xml">            <tree>                <field name="date"/>                <field name="of_id" />                <field name="moule" />                <field name="presse_id" />                <field name="qt_rebut"/>                <field name="defaut_id" />           </tree>        </field>    </record>    <record id="is_report_rebuts_graph_view" model="ir.ui.view">         <field name="name">is_report_rebuts_graph_view</field>         <field name="model">is.report.rebuts</field>         <field name="arch" type="xml">            <graph string="view" type="pivot">              <!--<field name="date" interval="week" type="row"/>-->              <field name="defaut_id"            type="col"/>              <field name="qt_rebut"             type="measure"/>             </graph>         </field>    </record>    <record id="is_report_rebuts_search_view" model="ir.ui.view">        <field name="name">is_report_rebuts_search_view</field>        <field name="model">is.report.rebuts</field>        <field name="arch" type="xml">            <search>                <!--                <filter icon="terp-go-home" name="ferme" string="Type Ferme" domain="[('type','=','ferme')]"/>                <filter icon="terp-go-home" name="previsionnel" string="Previsionnel" domain="[('type','=','previsionnel')]"/>                <field name="partner_id" context="{'contact_display':'partner'}"/>                -->                <field name="of_id" />                <field name="moule" />                <field name="presse_id" />                <field name="defaut_id" />                <group expand="1" string="Group By...">                    <filter name="group_of_id"     string="OF"     icon="terp-partner"              context="{'group_by':'of_id'}"/>                    <filter name="group_moule"     string="Moule"  icon="terp-gtk-jump-to-rtl"      context="{'group_by':'moule'}"/>                    <filter name="group_presse_id" string="Presse" icon="terp-accessories-archiver" context="{'group_by':'presse_id'}"/>                    <filter name="group_defaut_id" string="Défaut" icon="terp-gtk-jump-to-rtl"      context="{'group_by':'defaut_id'}"/>                    <filter                        string="Année"                                   context="{'group_by': 'date:year'}"/>                    <filter                        string="Mois"                                    context="{'group_by': 'date:month'}"/>                    <filter                        string="Semaine"                                 context="{'group_by': 'date:week'}"/>                </group>            </search>        </field>    </record>    <record id="is_report_rebuts_action" model="ir.actions.act_window">        <field name="name">Rebuts</field>        <field name="res_model">is.report.rebuts</field>        <field name="view_type">form</field>        <field name="view_mode">graph,tree</field>        <field name="search_view_id"  eval="False"/>    </record>    <!--<menuitem action="is_report_rebuts_action" id="menu_is_report_rebuts" parent="menu_is_cpi_indicateurs" sequence="10"/>--></data></openerp>